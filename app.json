{
  "name": "flash-travel-app",
  "displayName": "flash-travel-app",
  "expo": {
    "name": "Flash Travel",
    "slug": "flash-travel-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon/logo.png",
    "splash": {
      "image": "./src/assets/images/splash/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#393E46"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
